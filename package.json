  "license": "MIT",
  "author": "lio-mengxiang 微信: a2298613245",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "bin": {
    "mx": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin",
    "types"
